{
  "name": "distribumaster",
  "version": "1.0.0",
  "description": "DistribuMaster es una herramienta versátil y fácil de usar diseñada para estudiantes, investigadores y profesionales que necesitan calcular probabilidades, valores críticos y otras métricas relevantes para una variedad de distribuciones de probabilidad comunes.",
  "main": "server.js",
  "scripts": {
    "sass": "sass styles/sass/styles.sass:public/css/styles.css",
    "forms": "sass styles/sass/forms.sass:public/css/forms.css",
    "index": "sass styles/sass/index.sass:public/css/index.css",
    "layout": "sass styles/sass/layout.sass:public/css/layout.css",
    "historial-styles": "sass styles/sass/historial-styles.sass:public/css/historial-styles.css",
    "resultados": "sass styles/sass/resultados.sass:public/css/resultados.css",
    "distribuciones-style": "sass styles/sass/distribuciones-style.sass:public/css/distribuciones-style.css",
    "distribuciones-styles": "sass styles/sass/distribuciones-styles.sass:public/css/distribuciones-styles.css",
    "sass-watch": "sass --watch styles/sass:public/css",
    "start": "npm run sass && npm run index && npm run resultados && npm run historial-styles && npm run distribuciones-style && npm run distribuciones-styles && npm run layout && npm run forms && node server.js"
  },
  "keywords": [],
  "author": "Eliam Jesus Cauich Villanueva",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.7.2",
    "bcrypt": "^5.1.1",
    "blob-stream": "^0.1.3",
    
    "body-parser": "^1.20.2",
    "connect-flash": "^0.1.1",
    "connect-sqlite3": "^0.9.15",
    "cookie-parser": "^1.4.6",
    "docx": "^8.5.0",
    "dotenv": "^16.4.5",
    "echarts": "^5.5.1",
    "exceljs": "^4.4.0",
    "express": "^4.19.2",
    "express-rate-limit": "^7.3.1",
    "express-session": "^1.18.0",
    "file-saver": "^2.0.5",
    "html2canvas": "^1.4.1",
    "jsonwebtoken": "^9.0.2",
    "jspdf": "^2.5.1",
    "jstat": "^1.9.6",
    "mathjs": "^13.0.0",
    "mysql2": "^3.10.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pdfkit": "^0.15.0",
    "pug": "^3.0.3",
    "remixicon": "^4.2.0",
    "sass": "^1.77.5",
    "session": "^0.1.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.0"
  }
}
